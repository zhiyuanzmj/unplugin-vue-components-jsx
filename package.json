    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "unplugin-vue-components": "^0.27.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.2",
    "@antfu/ni": "^0.22.0",
    "@types/eslint": "^9.6.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.12",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.6.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
