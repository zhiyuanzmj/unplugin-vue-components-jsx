{
  "name": "unplugin-vue-components-jsx",
  "type": "module",
  "version": "0.27.4-2",
  "packageManager": "pnpm@9.14.2",
  "description": "Support JSX for unplugin-vue-components",
  "author": "zhiyuanzmj <zhiyuanzmj@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/unplugin-vue-components-jsx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiyuanzmj/unplugin-vue-components-jsx.git"
  },
  "bugs": "https://github.com/unplugin-vue-components-jsx/issues",
  "keywords": [
    "unplugin-vue-components",
    "jsx"
  ],
  "exports": {
    "./nuxt": "./lib/nuxt.js",
    "./rollup": "./lib/rollup.js",
    "./vite": "./lib/vite.js",
    "./vite.d": "./lib/vite.d.js",
    "./webpack": "./lib/webpack.js",
    "./rspack": "./lib/rspack.js",
    "./esbuild": "./lib/esbuild.js",
    "./resolvers": "./lib/resolvers.js",
    "./volar": {
      "import": "./dist/volar.js",
      "require": "./dist/volar.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "volar": [
        "./dist/volar.cjs"
      ],
      "*": [
        "./lib/*"
      ]
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "unplugin-vue-components": "^0.27.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@antfu/ni": "^0.23.1",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.9.4",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.14.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "ts-macro": "^0.1.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
